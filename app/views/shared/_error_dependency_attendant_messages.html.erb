<% if @dependency_attendant.errors.any? %>
<div id="error_explanation">
    <div class="alert alert-error">
        The form contains <%= pluralize(@dependency_attendant.errors.count,"error")%>.
    </div>
    <ul>
        <% @dependency_attendant.errors.full_messages.each do |msg| %>
        <li>* <%= msg %></li>
        <% end %>
    </ul>
</div>
<% end %>
