<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry"
        type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>
<div class="container-fluid">
    <div class="row-fluid">
        <%= provide(:title, 'Prueba') %>
        <h1>Consulta de plazas</h1>
        <p>Seleccione el tipo de busqueda
            <ul class="nav nav-tabs" id="myTab">
                <li class="active"><%= link_to "Tablas","#Tablas"%></li>
                <li><%= link_to "Mapas","#Mapas"%></li>
            </ul>
        </p>
        <div class="tab-content row span12">
            <div class="tab-pane active" id="Tablas">
                <%= render("plazas_tabla") %>
            </div>
                        
            <div class="tab-pane" id="Mapas">
                <%= render("plazas_mapa") %>
            </div>
        </div>
        <div class="row span12">
            <%= render("caja_seleccion") %>
        </div>
    </div>
</div>

